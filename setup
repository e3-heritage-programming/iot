cd client
sudo activator universal:package-bin

cd ..

cd Rest
sudo activator universal:package-bin

cd ..

\cp -rf start client
\cp -rf start Rest

docker build -t play .

docker run -d -v "`pwd`/client":/app:rw -p 9000:9000 --name client -t play
docker run -d -v "`pwd`/Rest":/app:rw -p 9001:9000 --name rest -t play

